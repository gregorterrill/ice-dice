{
  "require": {
    "craftcms/ckeditor": "3.5.1",
    "craftcms/cms": "4.5.3",
    "craftcms/feed-me": "5.2.0",
    "nystudio107/craft-seomatic": "4.0.30",
    "verbb/super-table": "3.0.9",
    "vlucas/phpdotenv": "^5.4.0",
    "modules/supersearch": "dev-master"
  },
  "require-dev": {
    "craftcms/generator": "^1.3.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "repositories": [
    {
      "type": "path",
      "url": "modules/supersearch"
    }
  ],
  "autoload": {
    "psr-4": {
      "modules\\supersearch\\": "modules/supersearch/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true,
      "php-http/discovery": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  }
}
